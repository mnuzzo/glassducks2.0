<p><%= review.rating %></p>
<p><%= review.comment %></p>

<% if user_signed_in? %>
  <% if review.user_id == current_user.id %>
  <%= link_to "Edit", edit_coop_review_path(review.coop, review) %>
  <%= link_to "Delete", coop_review_path(review.coop, review), method: :delete, data: {confirm: "Are you Sure?"} %> <br><br>

<% end %>
<% end %>
